{
	"version": 2,
	"name": "tsx-docs",
	"alias": "tsx-docs",
	"builds": [
		{
			"src": "docs/tsx-docs.config.js",
			"use": "petecorreia/now-tsx-docs",
			"config": { "staticDir": "docs/static" }
		}
	],
	"routes": [
		{
			"src": "^(.*)$",
			"dest": "docs$1"
		}
	],
	"regions": ["gru", "sfo", "bru"],
	"public": true
}
